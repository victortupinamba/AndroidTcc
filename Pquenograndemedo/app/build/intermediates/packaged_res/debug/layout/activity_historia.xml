<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/whatsapp_image_2023_10_22_at_19_57_45"
    tools:context=".historia">


    <TextView
        android:id="@+id/textHistoria"
        android:layout_width="300dp"
        android:layout_height="504dp"
        android:layout_marginTop="170dp"
        android:fontFamily="@font/ravi_prakash"
        android:text="@string/textEnredo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck" />

    <ImageButton
        android:id="@+id/imagebtnVoltar"
        android:layout_width="102dp"
        android:layout_height="59dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="157dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="#00FFFFFF"
        android:onClick="MAinActivity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton"
        app:layout_constraintStart_toStartOf="parent"

        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck,UseAppTint,ImageContrastCheck" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="98dp"
        android:layout_height="56dp"
        android:layout_marginTop="115dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="43dp"
        android:backgroundTint="#00FFFFFF"
        android:importantForAccessibility="no"
        android:tint="@color/invisivel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imagebtnVoltar"

        app:srcCompat="@android:drawable/divider_horizontal_bright"
        tools:ignore="ImageContrastCheck,MissingConstraints,UseAppTint" />

</androidx.constraintlayout.widget.ConstraintLayout>